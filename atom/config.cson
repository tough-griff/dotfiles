    viewedVersion: "0.94.0"
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
  "symbols-tree-view":
    scrollAnimation: false
  "Remote-FTP":
    hideLocalWhenDisplayed: true
  react: {}
".makefile.source":
  editor:
    tabLength: 4
