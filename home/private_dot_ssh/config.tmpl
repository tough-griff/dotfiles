Host *
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_ed25519
  IdentityFile ~/.ssh/id_rsa
{{- if eq .chezmoi.os "darwin" }}
  AddKeysToAgent yes
  UseKeychain yes
{{- end }}

Host github.com
  User git

{{ if not .ephemeral -}}
Host persephone 192.168.1.10
  HostName 192.168.1.10
  User pi
{{- end }}
