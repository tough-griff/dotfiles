# Usage: `brew bundle`
# `brew tap Homebrew/bundle` if uninstalled

# Install GNU core utilities
# see zshrc GNU-utils
brew "coreutils"
brew "moreutils"
brew "findutils"
brew "gnu-sed"

# Install other useful binaries
brew "casperjs", args: ["devel"]
brew "direnv"
brew "fasd"
brew "ghi"
brew "git"
brew "gpg"
brew "heroku-toolbelt"
brew "httpie"
brew "hub"
brew "node"
brew "openssl"
brew "p7zip"
brew "phantomjs192"
brew "pound"
brew "python"
brew "readline"
brew "redis"
brew "rename"
brew "the_silver_searcher"
brew "tree"
brew "wget", args: ["enable-iri"]
brew "zsh"
brew "zsh-completions"
brew "zsh-history-substring-search"
brew "zsh-syntax-highlighting"
